{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/map",
	"version": "0.1.0",
	"title": "Map",
	"category": "widgets",
	"icon": "location",
	"description": "Leaflet GeoJSON map based on data from a Google spreadsheet.",
	"example": {},
	"attributes": {
		"spreadsheetId": {
			"type": "string"
		},
		"sheetName": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"interactivity": true
	},
	"textdomain": "map",
	"editorScript": "file:./index.ts",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.ts"
}